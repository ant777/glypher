{
  "name": "Protemplater",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Uses the chrome.scripting API to inject JavaScript into web pages.",
  "background": {
    "service_worker": "sw.js"
  },
  "action": {
    "default_title": "Default Title",
    "default_popup": "popups/popup.html",
    "default_icon": {
      "32": "icons/32.png",
      "72": "icons/72.png",
      "128": "icons/128.png",
      "512": "icons/512.png"
    }
  },
  "permissions": ["scripting", "webNavigation", "storage", "tabs", "<all_urls>"],
  "host_permissions": ["https://example.com/*", "<all_urls>"]
}
